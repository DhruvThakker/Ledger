<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cyberknight.android.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountManagement.AccountManagementActivity"
            android:parentActivityName=".HomeScreen.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeScreen.MainActivity" />
        </activity>
    </application>

</manifest>